   <extension
         point="org.eclipse.ui.views">
      <category
            id="openbiomind.gui.views.category.OpenBiomind"
            name="OpenBiomind">
      </category>
      <view
            allowMultiple="false"
            category="openbiomind.gui.views.category.OpenBiomind"
            class="org.eclipse.ui.navigator.CommonNavigator"
            icon="icons/result_view.gif"
            id="openbiomind.gui.views.Results"
            name="Results"
            restorable="true">
         <description>
            Shows the results of the executed tasks.
         </description>
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="openbiomind.gui.preferences.OpenBiomindPreferencePage"
            id="openbiomind.gui.preferences.OpenBiomindPreferencePage"
            name="OpenBiomind">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="openbiomind.gui.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="openbiomind.gui.views.Results">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension></includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="openbiomind.gui.views.Results">
         <includes>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.linkHelper">
            </contentExtension>
            <contentExtension
                  pattern="OpenBiomind-GUI.contentExtension4">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="openbiomind.gui.editors.ImageEditor"
            default="true"
            extensions="png, PNG, gif, GIF, jpe, JPE, jpg, JPG, jpeg, JPEG"
            icon="icons/image_editor.gif"
            id="openbiomind.gui.editors.ImageEditor"
            name="Image Editor">
      </editor>
   </extension>

